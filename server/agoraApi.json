    "paths": {
        "/open/tags": {
            "get": {
                "tags": ["Tags"],
                "summary": "autogenerated for new tags, provide to update existing tag",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/Tags"
                        }
                    }
                }
            }
        }
        },
        "definitions": {
        "Tag": {
            "required": ["tag"],
            "properties": {
                "id": {
                    "type": "integer",
                    "description": "Assigned automatically for new tags, used to update existing tag if provided",
                    "uniqueItems": true
                },
                "tag": {
                    "type": "string",
                    "description": "The tag"
                },
                "ownedBy": {
                    "type": "integer",
                    "description": "Id of the user that first generated the tag"
                }
                
            },
            "example": {
                "id": 1,
                "tag": "Learning Python",
                "ownedBy": 1
            }
        },
        "Tags": {
            "type": "array",
            "$ref": "#/definitions/Tag"
        }
    }
